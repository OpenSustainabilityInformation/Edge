<form action="/impactAssessment/create" name="impactAssessment">
	<blank rules="">
        <type>hidden</type>
        <value>true</value>
    </blank>
	<fieldset name="Impact Assessment" multiple="yes" linked_type="bibo:impactAssessment">	
		<impactCategory rules="trim">
			<label>Impact Category</label>
			<type>lookup</type>
			<lookup>impact category</lookup>
			<linked_type>eco:impactCategory</linked_type>
		</impactCategory>
		<impactIndicator rules="trim">
			<label>Impact Indicator</label>
			<type>lookup</type>
			<lookup>impact indicator</lookup>
			<linked_type>eco:impactIndicator</linked_type>
		</impactIndicator>
		<assessmentQuantity rules="trim">
			<label>Quantity</label>
			<type>text</type>
			<linked_type>eco:assessmentQuantity</linked_type>
		</assessmentQuantity>		
		<assessmentUnit rules="trim">
			<label>Unit</label>
			<type>lookup</type>
			<lookup>unit</lookup>
			<linked_type>eco:assessmentUnit</linked_type>
		</assessmentUnit>		
	</fieldset>	
	    <submit rules="">
	        <type>submit</type>
	        <value>Submit</value>
	    </submit>
</form>