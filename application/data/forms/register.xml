<?xml version="1.0" encoding="UTF-8" ?>
<form action="/users/registered" name="">
<fieldset name="Register at Footprint.org">
	<registration_code rules="trim|xss_clean|required">
	  <label>Registration code</label>
      <type>text</type>
		<note>Footprinted is in closed beta version. If you want to test the site please send as an email.</note>
    </registration_code>
    <user_name rules="trim|xss_clean|required">
	  <label>User Name</label>
      <type>text</type>
    </user_name>
    <password rules="trim|xss_clean|required">
	  <label>Password</label>
      <type>password</type>
    </password>
    <email rules="trim|xss_clean|required">
	  <label>Email</label>
      <type>text</type>
    </email>
    <openid rules="trim|xss_clean">
      <type>text</type>
	  <label>Open ID</label>
    </openid>
    <foaf rules="trim|xss_clean">
	  <label>Friend of a friend URI</label>
	<type>text</type>
	  <note>A friend of a friend file is a semantic document that has your biographical information and relationships. Include the URI of yours here.</note>
    </foaf>
	<recaptcha>
	  <label>Recaptcha</label>
		<type>recaptcha</type>
	</recaptcha>
  <submit>
    <type>submit</type>
    <value>Submit</value>
  </submit>
</form>

