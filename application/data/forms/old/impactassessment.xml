<?xml version="1.0" encoding="UTF-8" ?>
<form action="info/create/lca/impactassessment" name="lca">
<fieldset name="Impact Assessment" linked_type="lca:impactAssessment">
  <fieldset name="Classification" linked_type="lca:classification">
    <impact_category rules="trim|xss_clean">
	  <label>Impact Category</label>
      <type>text</type>
	  <linked_type>lca:impactCategory</linked_type>
    </impact_category>
    <value rules="trim|xss_clean">
	  <label>Value</label>
      <type>text</type>
	  <linked_type>lca:impactCategoryValue</linked_type>
    </value>
    <unit rules="trim|xss_clean">
	  <label>Unit</label>
      <type>text</type>
	  <linked_type>lca:impactCategoryUnit</linked_type>
    </unit>
    <model rules="trim|xss_clean">
	  <label>Model Used</label>
      <type>text</type>
	  <linked_type>lca:impactCategoryModel</linked_type>
	  <multiple>yes</multiple>
    </model>
    <impacts rules="trim|xss_clean">
	  <label>Impacts</label>
      <type>text</type>
	  <linked_type>lca:impactCategoryImpacts</linked_type>
	  <multiple>yes</multiple>
    </impacts>
  </fieldset>
  <submit>
    <type>submit</type>
    <value>Submit</value>
  </submit>
</fieldset>
</form>

