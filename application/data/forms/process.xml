<form action="/lca/createpart/process" name="">
  <fieldset name="Process" linked_type="eco:models">
		<name rules="trim">
			<label>Name</label>
			<type>text</type>
		  	<linked_type>rdfs:label</linked_type>
		</name>	
		<datatype rules="trim">
			<label>Type</label>
			<type>hidden</type>
		  	<linked_type>rdfs:type</linked_type>
			<value>eco:Process</value>
		</datatype>	
		<UUID rules="trim">
			<label>UUID</label>
			<type>hidden</type>
		  	<linked_type>eco:hasUUID</linked_type>
		</UUID>
		<category rules="trim">
			<label>Category</label>
			<type>text</type>
		  	<linked_type>eco:hasCategory</linked_type>
			<multiple>yes</multiple>
		</category>
		<fieldset name="Valid Time Period" linked_type="eco:validInterval">
			<beginning rules="trim">
				<label>Beginning</label>
				<type>text</type>
			  	<linked_type>time:hasBeginning</linked_type>
			</beginning>
			<end rules="trim">
				<label>End</label>
				<type>text</type>
			  	<linked_type>time:hasEnd</linked_type>
			</end>
		</fieldset>
		<geoLocation rules="trim">
			<label>Geographic Location</label>
			<type>text</type>
		  	<linked_type>eco:hasGeoLocation</linked_type>
		</geoLocation>
	    <submit>
	        <type>submit</type>
	        <value>Submit</value>
	    </submit>
</fieldset>
</form>

