eco:ExactQuantity
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:hasValue ecoUD:zeroUncertaintyDistribution ;
                owl:onProperty eco:hasUncertaintyDistribution
              ] ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (eco:Quantity [ a       owl:Restriction ;
                            owl:hasValue ecoUD:zeroUncertaintyDistribution ;
                            owl:onProperty eco:hasUncertaintyDistribution
                          ])
              ] .

