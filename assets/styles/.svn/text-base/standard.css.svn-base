.clear {
	clear:both;
}

.shadow {
	-webkit-box-shadow: 4px 4px 4px rgba(0, 0, 0, 0.2);
	-moz-box-shadow: 4px 4px 4px rgba(0, 0, 0, 0.2);
	-o-box-shadow: 4px 4px 4px rgba(0, 0, 0, 0.2);
	border-shadow: 4px 4px 4px rgba(0, 0, 0, 0.2);
}

.inner-shadow {
	-webkit-box-shadow: inset 4px 4px 4px rgba(0, 0, 0, 0.2);
	-moz-box-shadow: inset 4px 4px 4px rgba(0, 0, 0, 0.2);
	-o-box-shadow: inset 4px 4px 4px rgba(0, 0, 0, 0.2);
	border-shadow: inset 4px 4px 4px rgba(0, 0, 0, 0.2);
}

.round {
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius:4px;
}